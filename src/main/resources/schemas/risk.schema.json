{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["riskScore", "level", "reasons"],
  "properties": {
    "riskScore": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "level": {
      "type": "string",
      "enum": [
        "LOW",
        "MEDIUM",
        "HIGH"
      ]
    },
    "reasons": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1
    }
  },
  "additionalProperties": true
}
